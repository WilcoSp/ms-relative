{
	"name": "ms-relative",
	"version": "0.1.0",
	"description": "MS utility library for formatting/parsing milliseconds to/from strings in most locales",
	"author": "Wilco Spruijt",
	"license": "MIT",
	"type": "module",
	"packageManager": "pnpm@^9.12.0",
	"repository": "https://github.com/WilcoSp/ms-relative",
	"homepage": "https://github.com/WilcoSp/ms-relative",
	"require": "dist/bundle.cjs",
	"browser": "dist/bundle.mjs",
	"import": "dist/index.mjs",
	"main": "dist/index.mjs",
	"typings": "dist/index.d.ts",
	"unpkg": "dist/bundle.mjs",
	"publishConfig": {
		"access": "public"
	},
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.mts",
				"default": "./dist/index.mjs"
			},
			"require": {
				"types": "./dist/bundle.d.ts",
				"default": "./dist/bundle.cjs"
			}
		}
	},
	"files": [
		"./dist"
	],
	"scripts": {
		"test": "vitest --run",
		"build": "tsup",
		"coverage": "vitest run --coverage",
		"tsLint": "tsc"
	},
	"keywords": [
		"ms",
		"relative",
		"ms-prettify",
		"ms-relative"
	],
	"devDependencies": {
		"@types/node": "^22.7.5",
		"@vitest/coverage-v8": "^2.1.3",
		"bumpp": "^9.7.1",
		"esbuild-plugin-file-path-extensions": "^2.1.3",
		"jsr": "^0.13.2",
		"tsup": "^8.3.0",
		"typescript": "^5.6.3",
		"vitest": "^2.1.3"
	}
}
